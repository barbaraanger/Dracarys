{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "src/index.js",
    "src/reportWebVitals.js",
    "src/setupTests.js",
    "webpack.config.js"
  ]
}